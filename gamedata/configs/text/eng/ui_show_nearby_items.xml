<?xml version="1.0" encoding="windows-1251"?>
<string_table>
	<string id="ui_mcm_menu_porky_show_nearby_items">
		<text>SNI - Show Nearby Items</text>
	</string>
	<string id="ui_mcm_porky_show_nearby_items_showNearbyItemsTitle">
		<text>SNI - Show Nearby Items</text>
	</string>
	<string id="ui_mcm_porky_show_nearby_items_showNearbyItemsEnabled">
		<text>Enable SNI - Show Nearby Items</text>
	</string>
	<string id="ui_mcm_porky_show_nearby_items_showNearbyItemsEnabled_desc">
		<text>Enables the "Show Nearby Items" feature, which allows the player to toggle on/off ground items UI windows.</text>
	</string>
	<string id="ui_mcm_porky_show_nearby_items_showNearbyItemMarkersEnabled">
		<text>Enable Item Name Markers</text>
	</string>
	<string id="ui_mcm_porky_show_nearby_items_showNearbyItemMarkersEnabled_desc">
		<text>Enables the item name to be shown when in range of the item on the ground.</text>
	</string>
	<string id="ui_mcm_porky_show_nearby_items_showNearbyItemWindowsEnabled">
		<text>Enable Item UI Window</text>
	</string>
	<string id="ui_mcm_porky_show_nearby_items_showNearbyItemWindowsEnabled_desc">
		<text>Enables the item UI Window to be shown when in range and player looks at the item on the ground.</text>
	</string>
    <string id="ui_mcm_porky_show_nearby_items_showNearbyItemsDetectionDistance">
        <text>Detection Distance for Show Nearby Items</text>
    </string>
	<string id="ui_mcm_porky_show_nearby_items_showNearbyItemsKey">
		<text>Keybind for SNI - Show Nearby Items</text>
	</string>
	<string id="ui_mcm_porky_show_nearby_items_showNearbyItemsKey_desc">
		<text>Enter the keybind you would like to use for toggling Show Nearby Items.</text>
	</string>
</string_table>